/* pages/home/home.wxss */
.container{
  padding:0
}
.lunbo{
  height:240px;
  width:100%;
}
.lunbo swiper-item{
  width:100%
}
.lunbo image{
  width:100%;
}
/* 九宫格 */
.grid{
  display:flex;
  flex-wrap: wrap;
}
.nav-item{
  height:200rpx;
  display:flex;
  width:20%;
  box-sizing: border-box;
  justify-content: center;
  align-items: center;
  flex-direction: column;
  border-bottom: 1px solid #f5f5f5;
  border-right:1px solid #f5f5f5;
  font-size: 32rpx;
}
.nav-item:nth-child(5),.nav-item:nth-child(10){
  border-right:0;
}
.nav-item image{
  width:102rpx;
  height:102rpx;
}
/* video */
.video{
  width:100%
}
